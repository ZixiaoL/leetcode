//Two Pointers
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int size = nums.size();
        if(size==0)return 0;
        int ans=1;
        for(int i=0;i<size;i++){
            if(nums[i]!=nums[ans-1]){
                nums[ans]=nums[i];
                ans++;
            }
        }
        return ans;
    }
};